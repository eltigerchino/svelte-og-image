{
	"name": "svelte-og-image",
	"version": "0.0.9",
	"license": "MIT",
	"author": {
		"name": "Chew Tee Ming",
		"email": "chewteeming01@gmail.com",
		"url": "https://github.com/eltigerchino"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eltigerchino/svelte-og-image.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "npm run prepack && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"dependencies": {
		"@vercel/og": "~0.7.2",
		"esm-env": "^1.2.2",
		"satori-html": "^0.3.2"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-vercel": "^5.8.1",
		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/package": "^2.4.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@types/eslint": "9.6.1",
		"@types/node": "^24.1.0",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"playwright": "^1.54.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.12",
		"svelte-check": "^4.3.0",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.0.0"
	},
	"keywords": [
		"svelte",
		"og",
		"image",
		"sveltekit",
		"svelte-kit",
		"svelte-og-image",
		"@vercel/og",
		"satori",
		"satori-html"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"patchedDependencies": {
			"@vercel/og": "patches/@vercel__og.patch"
		}
	},
	"packageManager": "pnpm@10.13.1"
}
